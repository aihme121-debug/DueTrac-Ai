{
  "name": "duetrack-ai",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "cd tests && npm test",
    "test:unit": "cd tests && npm run test:unit",
    "test:load": "cd tests && npm run test:load",
    "test:performance": "cd tests && npm run test:performance",
    "test:security": "cd tests && npm run test:security",
    "test:all": "cd tests && npm run test:all",
    "test:watch": "cd tests && npm run test:watch",
    "test:coverage": "cd tests && npm run test:coverage",
    "capacitor:init": "capacitor init",
    "capacitor:add:android": "capacitor add android",
    "capacitor:sync": "capacitor sync",
    "capacitor:open:android": "capacitor open android",
    "capacitor:build:android": "npm run build && capacitor sync android && cd android && ./gradlew assembleDebug",
    "mobile:setup": "npm run build && npx cap init \"DuetTrack AI\" \"com.duetrack.ai\" --web-dir=dist && npx cap add android",
    "mobile:build": "npm run build && npx cap sync && cd android && ./gradlew assembleDebug"
  },
  "dependencies": {
    "@capacitor/android": "^6.1.2",
    "@capacitor/core": "^6.1.2",
    "@capacitor/local-notifications": "^6.1.2",
    "@capacitor/push-notifications": "^6.0.3",
    "@capacitor/splash-screen": "^6.0.2",
    "@capacitor/status-bar": "^6.0.2",
    "@google/genai": "^0.3.0",
    "autoprefixer": "^10.4.16",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "firebase": "^10.7.1",
    "lucide-react": "^0.294.0",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.8.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^3.3.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@capacitor/cli": "^6.1.2",
    "@types/node": "^22.14.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitest/coverage-v8": "^1.0.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vitest": "^1.0.0"
  }
